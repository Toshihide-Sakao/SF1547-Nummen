f = @(y,t) sin(3*t) - 2*y;
y0 = 1.2;

T = 8; 

ns = [50 100 200 400];
iterations = length(ns);
results = {};
for i = 1:iterations
    n = ns(i);
    results{i} = zeros(n,1);
    h = T/n;
    y = y0;
    t = 0;
    for j = 0:n
        y = y + h*f(y,t);
        results{i}(j) = y;
        t = t + h;
    end
end
results
